{
  "name": "bankwiz_frontend",
  "version": "1.0.0-SNAPSHOT",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "build:dev": "run-p type-check \"build-only:dev {@}\" --",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e --config-file cypress.config.mts'",
    "test:component": "start-server-and-test preview http://localhost:4173 'cypress run --component --config-file cypress.config.mts'",
    "cy:open": "cypress open --config-file cypress.dev.config.mts",
    "build-only:dev": "vite build --mode development",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint:check": "eslint . --max-warnings 0",
    "format:check": "prettier --check src/",
    "lint:fix": "eslint . --fix",
    "format:fix": "prettier --write src/",
    "allformat": "run-s format:fix lint:fix",
    "clean:generated": "rm -rf src/generated/server",
    "generate:api": "openapi-generator-cli generate -i http://localhost:9595/v3/api-docs.yaml -g typescript-fetch -o ./src/generated/server",
    "generate": "run-s clean:generated generate:api"
  },
  "dependencies": {
    "@auth0/auth0-vue": "^2.3.3",
    "@primevue/themes": "^4.2.1",
    "chart.js": "^4.4.6",
    "primeicons": "^7.0.0",
    "primevue": "^4.2.1",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.15.3",
    "@primevue/auto-import-resolver": "^4.2.1",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.17.5",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitest/eslint-plugin": "1.1.7",
    "@vue/eslint-config-prettier": "^10.0.0",
    "@vue/eslint-config-typescript": "^14.1.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "cypress": "^13.15.0",
    "eslint": "^9.13.0",
    "eslint-plugin-cypress": "^4.0.0",
    "eslint-plugin-vue": "^9.29.0",
    "jsdom": "^25.0.1",
    "npm-run-all2": "^7.0.1",
    "prettier": "^3.3.3",
    "start-server-and-test": "^2.0.8",
    "typescript": "~5.6.0",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.10",
    "vite-plugin-vue-devtools": "^7.5.3",
    "vitest": "^2.1.3",
    "vue-tsc": "^2.1.6"
  }
}
